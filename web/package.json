{
  "name": "react-slingshot",
  "version": "1.1.1",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js",
    "build:watch": "npm run prebuild && babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js src/**/*.spec.js --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "auth0-lock": "^10.4.0",
    "babel-polyfill": "^6.3.14",
    "blueimp-load-image": "2.7.0",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "es6-promisify": "^5.0.0",
    "foundation-apps": "^1.2.0",
    "jwt-decode": "2.1.0",
    "lodash": "^4.2.1",
    "material-design-iconic-font": "^2.2.0",
    "moment": "2.15.1",
    "normalizr": "^2.0.0",
    "rc-tooltip": "3.4.2",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-addons-perf": "15.3.2",
    "react-addons-shallow-compare": "15.3.2",
    "react-dom": "15.3.2",
    "react-dropzone": "3.6.0",
    "react-helmet": "3.1.0",
    "react-masonry-component": "4.2.1",
    "react-notification-system": "0.2.10",
    "react-onclickoutside": "5.7.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "^4.0.2",
    "react-scroll": "1.4.1",
    "react-sticky": "5.0.5",
    "react-tether": "0.5.2",
    "react-waypoint": "3.1.3",
    "redux": "3.6.0",
    "redux-actions": "^0.12.0",
    "redux-form": "^6.1.0",
    "redux-logger": "2.7.0",
    "redux-saga": "^0.12.0",
    "superagent": "^2.3.0",
    "svg-inline-react": "1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.16.3",
    "browser-sync": "2.17.3",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "chokidar": "^1.6.1",
    "colors": "1.1.2",
    "cross-env": "3.1.2",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.8.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "http-proxy-middleware": "0.17.2",
    "imports-loader": "0.6.5",
    "install": "0.8.1",
    "mocha": "3.1.2",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.0",
    "postcss-loader": "0.13.0",
    "rc-tooltip": "3.4.2",
    "react-addons-test-utils": "15.3.2",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0"
  },
  "keywords:": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ]
}
